            <string>if(keyboard_check_pressed(keybinds_control.control_fire1) || first || skipthis)
{
    skipthis = false;
    instance_destroy(cutscene_fade_out_obj);
    first = false;
    if(ds_queue_empty(messages))
    {
        over = true;
    }
    else
    {
        var str = ds_queue_dequeue(messages);
        var len = string_length(str);
        if(string_char_at(str,1) == "C")
        {
            msg_spr = spr_carpathia_text;
            msg_text = string_copy(str,2,len-1);
        }
        else if(string_char_at(str,1) == "M")
        {
            msg_spr = spr_maria_text;
            msg_text = string_copy(str,2,len-1);
        }
        else if(string_char_at(str,1) == "m")
        {
            msg_spr = spr_maria_evil_text;
            msg_text = string_copy(str,2,len-1);
        }
        else if(string_char_at(str,1) == "B")
        {
            msg_spr = spr_blank_text;
            msg_text = string_copy(str,2,len-1);
        }
        else if(string_char_at(str,1) == "U")
        {
            msg_spr = spr_unknown_text;
            msg_text = string_copy(str,2,len-1);
        }
        else if(string_char_at(str,1) == "&gt;")
        {
            instance_create(0,0,cutscene_fade_out_obj);
        }
        else
        {
            msg_text = msg_text + "#" + string_copy(str,2,len-1)
        }
    }
}
else if(keyboard_check_pressed(keybinds_control.control_fire2))
{
    over = true;
}
